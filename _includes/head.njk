
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>{{ title | escape }}</title>
        <meta name="description" content="{{ description | escape }}" />
        <link rel="canonical" href="{{ page.url | escape }}" />
        <link rel="icon" type="image/x-icon" href="/_images/favicon.ico">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma/css/versions/bulma-no-dark-mode.min.css">
        <link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.min.css">
        {% if eleventy.env.runMode == "build" %}
            <link rel="stylesheet" href="/_css/custom.min.css">
        {% else %}
            <link rel="stylesheet" href="/_css/custom.css">
        {% endif %}
    </head>

